public class dog {

    public String name;
    public int age;

    public void setName(String name) {
        this.name = name;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public static void main(String[] args) {
        dog dog = new dog();
        dog.setName("Mike");
        dog.setAge(10);
        System.out.println(dog.name);
        System.out.println(dog.age);


    }
    
}
