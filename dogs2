public class Dogs {

    public String name;
    public int age;
    public String breed;


    public void setName(String name) {
        this.name = name;
    }

    public void setAge(int age) {
        this.age = age;
    }

    public void setBreed(String breed) {
        this.breed = breed;
    }

    public String getName() {
        return name;
    }

    public int getAge() {
        return age;
    }

    public String getBreed() {
        return breed;
    }

    public void talk() {
        switch (breed) {
            case "Rot":
                System.out.println("WOOF WOOF WOOF!");
                break;
            case "Chi":
                System.out.println("Small dog does woof!");
                break;
            case "Ger":
                System.out.println("Im a german!");
                break;

        }



    }

    public static void main(String[] args) {
        Dogs dog1 = new Dogs();    // ClassName varName = new TypeConstructor();
        dog1.setName("Mike");
        dog1.setAge(10);
        dog1.setBreed("Rottweiler");

        System.out.println(dog1.getName());
        System.out.println(dog1.getAge());
        System.out.println(dog1.getBreed());
        dog1.talk();

        Dogs dog2 = new Dogs();
        dog2.setName("Kuldar");
        dog2.setBreed("German");
        dog2.setAge(5);


        System.out.println(dog2.getName());
        System.out.println(dog2.getAge());
        System.out.println(dog2.getBreed());
        dog2.talk();

    }

}
